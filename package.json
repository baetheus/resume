{
  "name": "@baetheus/resume",
  "version": "1.2.0",
  "description": "Brandon Blaylock's personal resume",
  "main": "resume.tex",
  "private": true,
  "scripts": {
    "deps": "sudo pkgin in tex-latex-2016 tex-hyperref tex-xcolor tex-parskip tex-geometry tex-fancyhdr tex-etoolbox-2.5f rubber png",
    "build": "rubber --pdf resume && rubber --clean resume",
    "release": "npx standard-version",
    "test": "echo \"No tests implemented.\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baetheus/resume.git"
  },
  "keywords": [
    "resume",
    "brandon",
    "blaylock"
  ],
  "author": "Brandon Blaylock <brandon@null.pub> (https://blaylock.dev)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/baetheus/resume/issues"
  },
  "homepage": "https://github.com/baetheus/resume#readme",
  "devDependencies": {
    "standard-version": "^8.0.1"
  }
}
